<div class="card mb-3 w-50 mx-auto m-5 p-2">
    <img class="card-img-top" src="..." alt="{{Pet.Name}}">
    <div class="card-body">
        <h5 class="card-title">{{Pet.Name}}</h5>
        <p class="card-text">{{Pet.Age}} <br> {{Pet.Breed}} <br> {{Pet.Species}}</p>
        <p class="card-text"><small class="text-muted">{{date.added}}</small></p>
        <!-- Conditional Logic Attending /Unattend  -->
        <a href="#" class="btn btn-primary">Attending</a>
    </div>
</div>

